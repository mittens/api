{
  "name": "mittens-api",
  "private": true,
  "scripts": {
    "build": "tsc",
    "db": "prisma migrate dev",
    "dev": "env-cmd ts-node-dev --respawn --transpile-only --files src/index.ts",
    "lint": "eslint src --ext ts",
    "migrate": "env-cmd -f .env.production prisma migrate deploy",
    "prepare": "husky install",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@prisma/client": "2.19.0",
    "apollo-server-fastify": "^3.0.0-alpha.3",
    "axios": "^0.21.1",
    "bull-board": "^1.5.1",
    "bullmq": "^1.15.1",
    "class-validator": "^0.13.1",
    "date-fns": "^2.19.0",
    "fastify": "^3.14.1",
    "fastify-express": "^0.3.2",
    "firebase-admin": "^9.5.0",
    "graphql": "^15.5.0",
    "ioredis": "^4.24.5",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "husky": "^5.2.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "prisma": "2.19.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  }
}
